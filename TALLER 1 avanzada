#include <iostream>
#include <stdio.h>
#include <stdlib.h>
#include <time.h>
using namespace std;


int main(){

    int miarray [10];
    int *pointer = miarray;

    srand(time(NULL));
    int num;
     cout << "Valores" << "\t\t" << "Posicion en memoria" << endl;
    for (int i = 0 ; i < 10; i ++){
        num = (rand()%25);
        miarray[i] = num;
        cout << *(pointer + i) << "\t\t" << &miarray[i] << endl;

    }
    return 0;
}
 // segunda parte
 
 
